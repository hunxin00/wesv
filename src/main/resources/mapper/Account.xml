<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nh.wenet.nh.mapper.AccountMapper">

    <select id='selectAll' resultType="java.util.Map">
	select *  from wesv_account
  </select>

    <select id='selectinfoByUserName' resultType="java.util.Map" parameterType="java.lang.String">
	select *  from wesv_account where user_name=#{userName}
  </select>



</mapper>